{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "outDir": "./dist",
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitAny": false
  },
  "include": [
      "./src/**/*.ts",
  ]
}
